<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper

 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.mapper.UserMapper">

	<select id="selectUserLogin" parameterType="string" resultType="org.zerock.domain.User">
		SELECT 
			user_id as userId
			,user_pwd as userPwd
		FROM user WHERE user_id = c
	</select>
	
	
</mapper>